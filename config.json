{
    "source": ["output.json"],
    "platforms": {
      "css": {
        "transformGroup": "custom/css",
        "files": [
          {
            "destination": "css/variables.css",
            "format": "css/variables",
            "filter": "validToken"
          }
        ]
      },
      "json-flat": {
        "transformGroup": "web",
        "buildPath": "tokens/json-flat/",
        "files": [
          {
            "destination": "variables.json",
            "format": "json/flat"
          }
        ]
      },
      "js": {
        "transformGroup": "js",
        "buildPath": "tokens/js/",
        "files": [
          {
            "destination": "tokens.js",
            "format": "javascript/es6"
          }
        ]
      }
    }
  }